   "source": [
    "import itertools\n",
    "def subarray(a):\n",
    "    n = len(a)\n",
    "    indices = list(range(n+1))\n",
    "    for i,j in itertools.combinations(indices,2):\n",
    "        yield a[i:j]\n",
    "\n",
    "a=list(map(int,input().split()))\n",
    "l=list(subarray(a))\n",
    "print(l)"
   ]
  }
